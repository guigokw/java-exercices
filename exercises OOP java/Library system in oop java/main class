package POOjava;

public class MainLivro {
    public static void main(String[] args) {
        Livro livro1 = new Livro();
        Livro livro2 = new Livro();
        Livro livro3 = new Livro();
        Livro livro4 = new Livro();

        livro1.adicionarLivro();



        livro1.exibirDetalhesDoLivro();
        livro2.adicionarLivro();
        livro2.exibirDetalhesDoLivro();
        livro1.emprestarLivro();
        livro3.adicionarLivro();
        livro3.exibirDetalhesDoLivro();
        livro1.emprestarLivro();
        livro1.devolverLivro();
        livro2.devolverLivro();
        livro4.adicionarLivro();
        livro4.exibirDetalhesDoLivro();
    }
}
